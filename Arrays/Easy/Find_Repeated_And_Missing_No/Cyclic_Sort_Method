#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
vector<int>findMissingAndRepeatingNumber(vector<int>&nums){
    int n=nums.size();
    int i=0;
    while(i<n){
        int correctIdx=nums[i]-1;
        if(nums[i]!=nums[correctIdx]){
            swap(nums[i],nums[correctIdx]);
        } else{
            i++;
        }
    }
    for(int i=0;i<n;i++){
        if(nums[i]!=i+1){
            return{nums[i],i+1};
        }
    }
    return {-1,-1};
}
int main() {
    vector<int> nums = {3, 1, 2, 5, 4, 6, 7, 5};
    vector<int> result = findMissingAndRepeatingNumber(nums);
    for(int x : result){
        cout << x << " ";
    }
    return 0;
}
