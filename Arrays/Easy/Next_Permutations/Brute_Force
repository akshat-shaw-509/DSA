#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
void generatePerm(int idx,vector<int>&nums,vector<vector<int>>&all){
    if(idx==nums.size()){
        all.push_back(nums);
        return;
    }
    for(int i=idx;i<nums.size();i++){
        swap(nums[idx],nums[i]);
        generatePerm(idx+1,nums,all);
        swap(nums[idx],nums[i]);
    }
}
vector<int>nextPermutation(vector<int>nums){
    vector<vector<int>>all;
    generatePerm(0,nums,all);
    sort(all.begin(),all.end());
    int n=all.size();
    for(int i=0;i<n;i++){
        if(all[i]==nums){
            return all[(i+1)%n];
        }
    }
    return nums;
}
int main(){
    vector<int>nums={1,3,2};
    vector<vector<int>>all;
    generatePerm(0,nums,all);
    sort(all.begin(),all.end());
    for(auto &row:all){
        for(int x:row){
            cout<<x<<" ";
        }
        cout<<endl;
    }
}
